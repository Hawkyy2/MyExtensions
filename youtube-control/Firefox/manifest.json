{
"manifest_version": 2,
"name" : "Full Youtube controller",
"description" : "New trying Control Youtube play without visiting the tab.",

"homepage_url": "http://iamvp7.github.io/",
  "version": "2.0",

"icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions" : [
	"*://*.youtube.com/*",
	"tabs",
	"history",
	"activeTab"
	],
 "content_scripts": [
    {
        "matches" : ["*://*.youtube.com/*"],
        "js": ["popup/youtube.js"]
    }
    ],

  "browser_action": {
    "default_icon": {
      "19": "icons/icon-19.png",
      "38": "icons/icon-38.png"
    },
    "default_title": "New youtue Control your Youtube Player"
  }


}
