{
  "name": "Keyboard Shortcuts",
  "description": "Simple keyboard shortcut for fast work",
  "manifest_version": 2,
  "version": "1.0",
	"permissions" : [ "tabs"],
  "background": {
    "scripts": ["background.js"]
  },
 

  "commands": {
    "next-tab": {
      "suggested_key": { "default": "Ctrl+Right" },
      "description": "Jump to next tab"
    },
     "previous-tab": {
      "suggested_key": { "default": "Ctrl+Left" },
      "description": "Jump to next tab"
    }

  }
}

